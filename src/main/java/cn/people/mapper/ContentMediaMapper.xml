<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.people.mapper.ContentMediaMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.people.entity.ContentMedia">
    <result column="id" property="id" />
        <result column="user_id" property="userId" />
        <result column="content_id" property="contentId" />
        <result column="media_type" property="mediaType" />
        <result column="media_name" property="mediaName" />
        <result column="path" property="path" />
        <result column="desc" property="desc" />
        <result column="url" property="url" />
        <result column="level" property="level" />
        <result column="content_input_date" property="contentInputDate" />
        <result column="uploadtype" property="uploadtype" />
        <result column="title" property="title" />
        <result column="head_pic" property="headPic" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
        user_id, content_id, media_type, media_name, path, desc, url, level, content_input_date, uploadtype, title, head_pic
    </sql>

</mapper>
