<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.people.mapper.SplitModMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.people.entity.SplitMod">
    <result column="id" property="id" />
        <result column="split_id" property="splitId" />
        <result column="mod_name" property="modName" />
        <result column="mod_des" property="modDes" />
        <result column="mod_html" property="modHtml" />
        <result column="create_time" property="createTime" />
        <result column="create_id" property="createId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
        split_id, mod_name, mod_des, mod_html, create_time, create_id
    </sql>

</mapper>
