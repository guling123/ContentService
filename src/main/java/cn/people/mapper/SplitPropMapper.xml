<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.people.mapper.SplitPropMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.people.entity.SplitProp">
    <result column="id" property="id" />
        <result column="split_id" property="splitId" />
        <result column="prop_name" property="propName" />
        <result column="prop_des" property="propDes" />
        <result column="prop_value" property="propValue" />
        <result column="prop_type" property="propType" />
        <result column="prop_level" property="propLevel" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
        split_id, prop_name, prop_des, prop_value, prop_type, prop_level
    </sql>

</mapper>
