<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.people.mapper.SplitPropValueMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.people.entity.SplitPropValue">
    <result column="id" property="id" />
        <result column="ttp_id" property="ttpId" />
        <result column="channel_id" property="channelId" />
        <result column="prop_value" property="propValue" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
        ttp_id, channel_id, prop_value
    </sql>

</mapper>
